<div class="row" style="height:91vh; background-color:rgba(0,0,0,0.9);">
  <div class="col s12 m6 l4 push-m3 push-l4">
    <div class="card-panel" style="margin-top:25px;">
      <h2 class="center-align">Register</h2>
      {{message}}
      <form method="POST" action="">
      <div>
        <label>First Name:</label> <input type="text" name="fname">
      </div>
      <div>
        <label>Last Name:</label> <input type="text" name="lname">
      </div>
      <div>
        <label>Email:</label> <input type="email" name="username"> 
      </div>
      <div>
        <label>Password:</label> <input type="password" name="password"> 
      </div>
        <input type="submit" class="btn-large waves-effect waves-light green" style="width:100%;" value="Go"> 
      </form>
    </div>
  </div>
</div>